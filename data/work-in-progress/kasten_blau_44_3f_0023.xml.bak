   <text type="letter">
   <body>
  <div>
    <pb facs="#facs_1" n="1" xml:id="Kasten_blau_44_3f_0023.jpg"/>
    <ab facs="#facs_1_r1">
      <lb facs="#facs_1_r1l1" n="N001"/>roser a
      <lb facs="#facs_1_r1l2" n="N002"/>EeDh
      <lb facs="#facs_1_r1l3" n="N003"/>ohre age  die cagseti
      <lb facs="#facs_1_r1l4" n="N004"/>
      <lb facs="#facs_1_r1l5" n="N005"/>duer ageh aag wahe ich vndt ie
      <lb facs="#facs_1_r1l6" n="N006"/>gehorsambstem dankh das lize gübt
      <lb facs="#facs_1_r1l7" n="N007"/>e
      <lb facs="#facs_1_r1l8" n="N008"/>welhe cospolation vnsers beads
      <lb facs="#facs_1_r1l9" n="N009"/>den pfal grauen darl epliga 6
      <lb facs="#facs_1_r1l10" n="N010"/>vnd ersorderung vners hurhades
      <lb facs="#facs_1_r1l11" n="N011"/>intehe durch dero allersochste
      <lb facs="#facs_1_r1l12" n="N012"/>autorirtel suoubl t
      <lb facs="#facs_1_r1l13" n="N013"/>vollegn das ihr aajper bligste
      <lb facs="#facs_1_r1l14" n="N014"/>mein alle ister ägser vnd er
      <lb facs="#facs_1_r1l15" n="N015"/>hhreo lebten gungst sanherzeiticg ver
      <lb facs="#facs_1_r1l16" n="N016"/>storbr gemahl on rebm
      <lb facs="#facs_1_r1l17" n="N017"/>imesin vnd das diespelbe beiy schoren
      <lb facs="#facs_1_r1l18" n="N018"/>seeeesiono tectele eb puoenis
      <lb facs="#facs_1_r1l19" n="N019"/>ali bäftigist behanbtes vnd man
      <lb facs="#facs_1_r1l20" n="N020"/>temirrt das janso laiela vnd lediil
      <lb facs="#facs_1_r1l21" n="N021"/>don aller angenmahtiger at snduch
      <lb facs="#facs_1_r1l22" n="N022"/>abghalten an dem eig lchtaus ver
      <lb facs="#facs_1_r1l23" n="N023"/>diem vnd die bein dauen in gs
      <lb facs="#facs_1_r1l24" n="N024"/>berwghmb lia gelegenben
      <lb facs="#facs_1_r1l25" n="N025"/>grstenthumberen vnd landen befindlichen
      <lb facs="#facs_1_r1l26" n="N026"/>comendentes ecoroni in ho b
      <lb facs="#facs_1_r1l27" n="N027"/>kflichten e contimir oder in
      <lb facs="#facs_1_r1l28" n="N028"/>rweik solche veribli obigen faiche
      <lb facs="#facs_1_r1l29" n="N029"/>heütigen laökslahls tos erlaschen gehalten
      <lb facs="#facs_1_r1l30" n="N030"/>wrerden wilte infebige aus da
      <lb facs="#facs_1_r1l31" n="N031"/>heute bey angosn 
      <lb facs="#facs_1_r1l32" n="N032"/>mogen aluch durch eigne abschükhich
      <lb facs="#facs_1_r1l33" n="N033"/>bey meig velischen post mit
      <lb facs="#facs_1_r1l34" n="N034"/>nichtekh angenehug sich vehokuent
      <lb facs="#facs_1_r1l35" n="N035"/>vnd weil an dem allel gelagen
      <lb facs="#facs_1_r1l36" n="N036"/>das sehhane abschukhuung bosten vnd
      <lb facs="#facs_1_r1l37" n="N037"/>hn allpenstandt beschlaniget werde
      <lb facs="#facs_1_r1l38" n="N038"/></ab>
    <pb facs="#facs_2" n="2" xml:id="Kasten_blau_44_3f_0024.jpg"/>
    <ab facs="#facs_2_r2">
      <lb facs="#facs_2_r2l1" n="N001"/>he bitte suro maser gs
      <lb facs="#facs_2_r2l2" n="N002"/>gesorpombst ziz belieben Diepelbe
      <lb facs="#facs_2_r2l3" n="N003"/>solches gestaltsen gubsorderdas
      <lb facs="#facs_2_r2l4" n="N004"/>durch dero herzogng gmelt vnb
      <lb facs="#facs_2_r2l5" n="N005"/>bruederu en vnd dero iumcasin vai
      <lb facs="#facs_2_r2l6" n="N006"/>vwider bringlicher hen etwisstag
      <lb facs="#facs_2_r2l7" n="N007"/>v ho ven, das strrichheng pstection
