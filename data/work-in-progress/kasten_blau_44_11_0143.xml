      <lb facs="#facs_1_r1l3" n="N003"/>eede
      <lb facs="#facs_1_r1l4" n="N004"/>aller durchleüchtigster s
      <lb facs="#facs_1_r1l5" n="N005"/>der venmilt ibter layser
      <lb facs="#facs_1_r1l6" n="N006"/>ceono m
      <lb facs="#facs_1_r1l7" n="N007"/>dunach vnter ihr M da
      <lb facs="#facs_1_r1l8" n="N008"/>ahn stalt min enhendige
      <lb facs="#facs_1_r1l9" n="N009"/>ihn mich haltenden agenhandge
      <lb facs="#facs_1_r1l10" n="N010"/>schreibeus
      <lb facs="#facs_1_r1l11" n="N011"/>wrrrt hiebeenmetes deo
      <lb facs="#facs_1_r1l12" n="N012"/>dem ansechem noch ahn der degen
      <lb facs="#facs_1_r1l13" n="N013"/>mnem lagseirm ata gewich
      <lb facs="#facs_1_r1l14" n="N014"/>leters handt schreiben dahieim ven
      <lb facs="#facs_1_r1l15" n="N015"/>müthlich durch einem von tost
      <lb facs="#facs_1_r1l16" n="N016"/> eingelan den ist also habe
      <lb facs="#facs_1_r1l17" n="N017"/>meinem schuldigkeit genmmeß zu
      <lb facs="#facs_1_r1l18" n="N018"/>r zue gleig vndeh
      <lb facs="#facs_1_r1l19" n="N019"/>em
      <lb facs="#facs_1_r1l20" n="N020"/>fe
      <lb facs="#facs_1_r1l21" n="N021"/>amb vergebulg 
      <lb facs="#facs_1_r1l22" n="N022"/>sein eachtet solches 
      <lb facs="#facs_1_r1l23" n="N023"/>gegenwaes wls
      <lb facs="#facs_1_r1l24" n="N024"/>agenfei gurshlreiben mig mß hinit gehr
      <lb facs="#facs_1_r1l25" n="N025"/>lambst ein din schriben vnd zu
      <lb facs="#facs_1_r1l26" n="N026"/>vernag akas ve
      <lb facs="#facs_1_r1l27" n="N027"/>m alseb der mi ofe
      <lb facs="#facs_1_r1l28" n="N028"/>dem beharelichem vohm hulde
      <lb facs="#facs_1_r1l29" n="N029"/>lelgst gefallener bals
      <lb facs="#facs_1_r1l30" n="N030"/>vnd gnadn mich vnts zu
      <lb facs="#facs_1_r1l31" n="N031"/>tasen wer ich nach
      <lb facs="#facs_1_r1l32" n="N032"/>endehlem in vinfeße rene
      <lb facs="#facs_1_r1l33" n="N033"/>mih etwas enomodi
      <lb facs="#facs_1_r1l34" n="N034"/>befintncht zugelasen etien vergleidendisl
      <lb facs="#facs_1_r1l35" n="N035"/>di bt dulig 11
      <lb facs="#facs_1_r1l36" n="N036"/> solhe  fe
      <lb facs="#facs_1_r1l37" n="N037"/>v vo  a a
      <lb facs="#facs_1_r1l38" n="N038"/>cprese veren werden
      <lb facs="#facs_1_r1l39" n="N039"/>
