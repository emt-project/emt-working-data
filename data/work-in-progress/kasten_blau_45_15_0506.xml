   <text type="letter">
   <body>
  <div>
    <pb facs="#facs_1" n="285 r" xml:id="Kasten_blau_45_15_0506.jpg"/>
    <ab facs="#facs_1_r1">
      <lb facs="#facs_1_r1l1" n="N001"/>ihr m die digstrie
      <lb facs="#facs_1_r1l2" n="N002"/>mit dise
      <lb facs="#facs_1_r1l3" n="N003"/>T
      <lb facs="#facs_1_r1l4" n="N004"/>iglihwochten
      <lb facs="#facs_1_r1l5" n="N005"/> mban sen
      <lb facs="#facs_1_r1l6" n="N006"/>so deonischten</ab>
    <ab facs="#facs_1_r2">
      <lb facs="#facs_1_r2l1" n="N001"/>allerdurchleüchtigster
      <lb facs="#facs_1_r2l2" n="N002"/>dnr e Mma haen zuwich angeri in
      <lb facs="#facs_1_r2l3" n="N003"/>huldiche allergßen schriben vom i tendt
      <lb facs="#facs_1_r2l4" n="N004"/>in dis srd h harbandtwöchen vus
      <lb facs="#facs_1_r2l5" n="N005"/>dolchen braden minlrichts bebersen trndt
      <lb facs="#facs_1_r2l6" n="N006"/>inmschteb zutirmuriren gemesen
      <lb facs="#facs_1_r2l7" n="N007"/>so dah bendte iha dlg  ma
      <lb facs="#facs_1_r2l8" n="N008"/>Ma gott sin arig gedankhtn sib
      <lb facs="#facs_1_r2l9" n="N009"/>in gans gesenidtern fndt vn wehl
      <lb facs="#facs_1_r2l10" n="N010"/>standt befindtag der allerschöhste wollen
