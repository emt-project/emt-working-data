    <ab facs="#facs_1_TextRegion_1638456921795_563">
      <lb facs="#facs_1_r2l1" n="N001"/>alledurchleüchtigester r.
      <lb facs="#facs_1_r2l2" n="N002"/>aucn . a ere ater
      <lb facs="#facs_1_r2l3" n="N003"/>vn ditero schsezeuiltiges daerawat de
      <lb facs="#facs_1_r2l4" n="N004"/>collmichem officidem habteen diehen
      <lb facs="#facs_1_r2l5" n="N005"/>famsbnen zuemkel dnseim abergüädige
      <lb facs="#facs_1_r2l6" n="N006"/>zu vernkiheim gemihein meltns deas duen
      <lb facs="#facs_1_r2l7" n="N007"/>sere darburch die asierintie der derng
      <lb facs="#facs_1_r2l8" n="N008"/>disulat auch zu fommer prcorration
      <lb facs="#facs_1_r2l9" n="N009"/>mein mihreer hoffnung erhalten ase
      <lb facs="#facs_1_r2l10" n="N010"/>einmit denfelter auch angebe, der
      <lb facs="#facs_1_r2l11" n="N011"/>wbey derolnket derro vmicht   al vene
      <lb facs="#facs_1_r2l12" n="N012"/>efwarenfeldt naderwerthin 2ppti
      <lb facs="#facs_1_r2l13" n="N013"/>mie wenden sol zu er. k.ig zu rrinegle 
      <lb facs="#facs_1_r2l14" n="N014"/>seinem allenvndehänigster zuflinihten
      <lb facs="#facs_1_r2l15" n="N015"/>mit dero allewgehorsambstee bibt dern
      <lb facs="#facs_1_r2l16" n="N016"/>El
      <lb facs="#facs_1_r2l17" n="N017"/>alte möchlin daro mlde auch in de
      <lb facs="#facs_1_r2l18" n="N018"/>
      <lb facs="#facs_1_r2l19" n="N019"/>g
      <lb facs="#facs_1_r2l20" n="N020"/>hsern das durch ihet allerschicht
      <lb facs="#facs_1_r2l21" n="N021"/>armiltebung serer zueretet au ade
      <lb facs="#facs_1_r2l22" n="N022"/>kimen iftng fefll de aoi
      <lb facs="#facs_1_r2l23" n="N023"/>ritration merfn discalati wahr
      <lb facs="#facs_1_r2l24" n="N024"/>weil wiende verde weilen er zu solch
      <lb facs="#facs_1_r2l25" n="N025"/>mache mit auch belanget, brey ltin
      <lb facs="#facs_1_r2l26" n="N026"/>men minuen dndenistion fergste
      <lb facs="#facs_1_r2l27" n="N027"/>ein wenderng dro hab duoch disinen dann
      <lb facs="#facs_1_r2l28" n="N028"/>ein enfefung emzgang rdn fficiali
      <lb facs="#facs_1_r2l29" n="N029"/>zu imm ausder besaubdig kraa
      <lb facs="#facs_1_r2l30" n="N030"/>deuotion miht ei farden gohren
      <lb facs="#facs_1_r2l31" n="N031"/>mellem vende bibe solchem nach
      <lb facs="#facs_1_r2l32" n="N032"/>ew. May. may freriis gehorsambn
      <lb facs="#facs_1_r2l33" n="N033"/>die grmisten kueste derin porpatimten
      <lb facs="#facs_1_r2l34" n="N034"/>mein bestengegale helten miht
      <lb facs="#facs_1_r2l35" n="N035"/>ein angndenw sondern abs d sust vnd
      <lb facs="#facs_1_r2l36" n="N036"/>h
      <lb facs="#facs_1_r2l37" n="N037"/>esmen las desem protient vnd vereen
      <lb facs="#facs_1_r2l38" n="N038"/>anrlation rrset worene möge
      <lb facs="#facs_1_r2l39" n="N039"/>eben bise uutir
      <lb facs="#facs_1_r2l40" n="N040"/>verbeenst dich iehr
      <lb facs="#facs_1_r2l41" n="N041"/>Eleonora
      <lb facs="#facs_1_r2l42" n="N042"/>mit mit keif em vespeet derchanen
      <lb facs="#facs_1_r2l43" n="N043"/>p nenoburg dem pb. amaniy bs
      <lb facs="#facs_1_r1l1" n="N044"/>vo
      <lb facs="#facs_1_r1l2" n="N045"/>eishenm maisin dein kanziernide
      <lb facs="#facs_1_r1l3" n="N046"/>frsa 
      <lb facs="#facs_1_r1l4" n="N047"/>i maner also anch zu seinars
      <lb facs="#facs_1_r1l5" n="N048"/>wie bien 
      <lb facs="#facs_1_r1l6" n="N049"/>r</ab>
