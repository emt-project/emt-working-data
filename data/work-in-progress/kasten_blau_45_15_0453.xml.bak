   <text type="letter">
   <body>
  <div>
    <pb facs="#facs_1" n="254 r" xml:id="Kasten_blau_45_15_0453.jpg"/>
    <ab facs="#facs_1_r1">
      <lb facs="#facs_1_r1l1" n="N001"/>ihn adin ri
      <lb facs="#facs_1_r1l2" n="N002"/>die dröslichencor
      <lb facs="#facs_1_r1l3" n="N003"/>timation dero
      <lb facs="#facs_1_r1l4" n="N004"/>den allerlebsten
      <lb facs="#facs_1_r1l5" n="N005"/></ab>
    <ab facs="#facs_1_r2">
      <lb facs="#facs_1_r2l1" n="N001"/>allenl
      <lb facs="#facs_1_r2l2" n="N002"/>aus d h mg ngenhundtigen
      <lb facs="#facs_1_r2l3" n="N003"/>allengsten schriben tron pessation haben
      <lb facs="#facs_1_r2l4" n="N004"/>mit höchsteren srüdern derosordirturerid
      <lb facs="#facs_1_r2l5" n="N005"/>gesengnden stredt ze seden nehlst andt te
      <lb facs="#facs_1_r2l6" n="N006"/>monen dardit deron allmaultige
      <lb facs="#facs_1_r2l7" n="N007"/>olten vaznitlils zudemohrge brud geister
      <lb facs="#facs_1_r2l8" n="N008"/>digs zem billen das r ehnigs ehrigen
      <lb facs="#facs_1_r2l9" n="N009"/>mag malg brudseibs sag ihrero mah
      <lb facs="#facs_1_r2l10" n="N010"/>derin btilin lngigen tent tebrnd geden
      <lb facs="#facs_1_r2l11" n="N011"/>og gra gerrschafften leigen engerle
      <lb facs="#facs_1_r2l12" n="N012"/>gnaden laub cnitndion aerlich
      <lb facs="#facs_1_r2l13" n="N013"/>consecienen wollen den vesirsigern
      <lb facs="#facs_1_r2l14" n="N014"/>llendtn sendt bedrüelten zusendt be
      <lb facs="#facs_1_r2l15" n="N015"/>d stendtn alle mine guotshil
      <lb facs="#facs_1_r2l16" n="N016"/>lendten aiten tir od in aflirte derl
      <lb facs="#facs_1_r2l17" n="N017"/>leden treidt docb nit genßen bemeiths in daben
      <lb facs="#facs_1_r2l18" n="N018"/>gsis hem gehten schulig gb verg
      <lb facs="#facs_1_r2l19" n="N019"/>ist aih belagent srigen besechten, das
      <lb facs="#facs_1_r2l20" n="N020"/>comon gericht wndt beralten die es
      <lb facs="#facs_1_r2l21" n="N021"/>drapsen heernter gedhon teudt frnch
