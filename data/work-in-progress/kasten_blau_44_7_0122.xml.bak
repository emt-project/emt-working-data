    <ab facs="#facs_1_r1">
      <lb facs="#facs_1_r1l1" n="N001"/>E
      <lb facs="#facs_1_r1l2" n="N002"/>ue
      <lb facs="#facs_1_r1l3" n="N003"/>hne ma die baysein
      <lb facs="#facs_1_r1l4" n="N004"/>da
      <lb facs="#facs_1_r1l5" n="N005"/>enien vu v</ab>
    <ab facs="#facs_1_r2">
      <lb facs="#facs_1_r2l1" n="N001"/>aich baller göste aschringen
      <lb facs="#facs_1_r2l2" n="N002"/>gmusem f ma  aus
      <lb facs="#facs_1_r2l3" n="N003"/>deru bnschs he enstehen da
      <lb facs="#facs_1_r2l4" n="N004"/>ergen berder wdtrahen zu
      <lb facs="#facs_1_r2l5" n="N005"/>besten visel vnd dag
      <lb facs="#facs_1_r2l6" n="N006"/>dochteein mein gekemeha
      <lb facs="#facs_1_r2l7" n="N007"/>denm von degenseldtan mich
      <lb facs="#facs_1_r2l8" n="N008"/>berichtet weie doch ihme
      <lb facs="#facs_1_r2l9" n="N009"/>der ventewste be weiteen
      <lb facs="#facs_1_r2l10" n="N010"/>varliht s allewest
      <lb facs="#facs_1_r2l11" n="N011"/>se befonderen vnd mhne
      <lb facs="#facs_1_r2l12" n="N012"/>zeit verlst zu aberschinlichen
      <lb facs="#facs_1_r2l13" n="N013"/>gmesten dse zaben solle
      <lb facs="#facs_1_r2l14" n="N014"/>lchser selben zu  kdig
      <lb facs="#facs_1_r2l15" n="N015"/>sallergdsei velieben obdie
      <lb facs="#facs_1_r2l16" n="N016"/>selbe bis dahn die duden
      <lb facs="#facs_1_r2l17" n="N017"/>lise velerbrng vnd feintst
      <lb facs="#facs_1_r2l18" n="N018"/>abeedt aus sellen oder weßen
      <lb facs="#facs_1_r2l19" n="N019"/>aurh sorchten allen dse we
      <lb facs="#facs_1_r2l20" n="N020"/>seen wolden de dich mi f
      <lb facs="#facs_1_r2l21" n="N021"/>ma stnd dero vrdig
      <lb facs="#facs_1_r2l22" n="N022"/>in portegall hiern fi
      <lb facs="#facs_1_r2l23" n="N023"/>mnde cntention vuteffeti
      <lb facs="#facs_1_r2l24" n="N024"/>den eufgists angelegen
      <lb facs="#facs_1_r2l25" n="N025"/>sein diten vde t
      <lb facs="#facs_1_r2l26" n="N026"/>l en e den 2ten a
      <lb facs="#facs_1_r2l27" n="N027"/>
      <lb facs="#facs_1_r2l28" n="N028"/>wer Durchlauchtse
      <lb facs="#facs_1_r2l29" n="N029"/>een
      <lb facs="#facs_1_r2l30" n="N030"/>EeMLe
      <lb facs="#facs_1_r2l31" n="N031"/>ewur</ab>
  </div>
</body>

