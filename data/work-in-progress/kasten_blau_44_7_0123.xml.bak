   <text type="letter">
   <body>
  <div>
    <pb facs="#facs_1" n="69 r" xml:id="Kasten_blau_44_7_0123.jpg"/>
    <ab facs="#facs_1_r1">
      <lb facs="#facs_1_r1l1" n="N001"/>
      <lb facs="#facs_1_r1l2" n="N002"/>dadurchleüchtigster
      <lb facs="#facs_1_r1l3" n="N003"/>Ehr  M
      <lb facs="#facs_1_r1l4" n="N004"/>beleilen befins dis dies seht
      <lb facs="#facs_1_r1l5" n="N005"/>diear
      <lb facs="#facs_1_r1l6" n="N006"/>
      <lb facs="#facs_1_r1l7" n="N007"/>hi sahsem beidn  voe
      <lb facs="#facs_1_r1l8" n="N008"/>mich venfehren besehen
      <lb facs="#facs_1_r1l9" n="N009"/>llem bese nachkelg vor
      <lb facs="#facs_1_r1l10" n="N010"/>er may M m
      <lb facs="#facs_1_r1l11" n="N011"/>allerzigstien velser mich heg
      <lb facs="#facs_1_r1l12" n="N012"/>m der gememe werse
      <lb facs="#facs_1_r1l13" n="N013"/>heben dißen hab ich mich
      <lb facs="#facs_1_r1l14" n="N014"/>verknden drhe alle
      <lb facs="#facs_1_r1l15" n="N015"/>föchst ged vben bay
      <lb facs="#facs_1_r1l16" n="N016"/>mag in mes der anla
      <lb facs="#facs_1_r1l17" n="N017"/>disen in vsend 
      <lb facs="#facs_1_r1l18" n="N018"/>krawen di aoentirren
      <lb facs="#facs_1_r1l19" n="N019"/>er M ma vndt
      <lb facs="#facs_1_r1l20" n="N020"/>ehrangst gehrsambste
      <lb facs="#facs_1_r1l21" n="N021"/>billemt dies elle geesen
      <lb facs="#facs_1_r1l22" n="N022"/>shame mee verwg
      <lb facs="#facs_1_r1l23" n="N023"/> mte nnmerng be
      <lb facs="#facs_1_r1l24" n="N024"/>allerhochsteged her a
      <lb facs="#facs_1_r1l25" n="N025"/> vnd dien sichen dorse
      <lb facs="#facs_1_r1l26" n="N026"/>Ma besten zi enks hil
      <lb facs="#facs_1_r1l27" n="N027"/>n m rsen du
      <lb facs="#facs_1_r1l28" n="N028"/>fierbenten bedn sonderchtie
      <lb facs="#facs_1_r1l29" n="N029"/>engen hie veka
      <lb facs="#facs_1_r1l30" n="N030"/>edaken dohi zu
      <lb facs="#facs_1_r1l31" n="N031"/>vernle das die lilich disela
      <lb facs="#facs_1_r1l32" n="N032"/>ken mögs degb
      <lb facs="#facs_1_r1l33" n="N033"/>soniton daser e 
      <lb facs="#facs_1_r1l34" n="N034"/>solen zu beühign seg mi
      <lb facs="#facs_1_r1l35" n="N035"/>ne 
      <lb facs="#facs_1_r1l36" n="N036"/>ebnelrenst herr
      <lb facs="#facs_1_r1l37" n="N037"/> atertragen ihn ede
      <lb facs="#facs_1_r1l38" n="N038"/>E
      <lb facs="#facs_1_r1l39" n="N039"/>ntlis mit inge sole
      <lb facs="#facs_1_r1l40" n="N040"/>in mseestendtmsen vnd
      <lb facs="#facs_1_r1l41" n="N041"/>hab gelengene alah vnd gelegenheit
      <lb facs="#facs_1_r1l42" n="N042"/>geben derster
      <lb facs="#facs_1_r1l43" n="N043"/>E</ab>
  </div>
</body>

