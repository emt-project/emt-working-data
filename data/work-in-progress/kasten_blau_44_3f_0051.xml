    <ab facs="#facs_1_r3">
      <lb facs="#facs_1_r3l1" n="N001"/>alladdehlahtehte
      <lb facs="#facs_1_r3l2" n="N002"/>o ase daen so bois 
      <lb facs="#facs_1_r3l3" n="N003"/>kamt mit wes pronte ma
      <lb facs="#facs_1_r3l4" n="N004"/>erbor nach dm deso h
      <lb facs="#facs_1_r3l5" n="N005"/>ve zu vnte en diemster
      <lb facs="#facs_1_r3l6" n="N006"/>auffarichtem abernomme
      <lb facs="#facs_1_r3l7" n="N007"/>cacriment verschrne nge
      <lb facs="#facs_1_r3l8" n="N008"/>vrter vehinaung halbe
      <lb facs="#facs_1_r3l9" n="N009"/>von agenwar i cmpepm
