   <text type="letter">
   <body>
  <div>
    <pb facs="#facs_1" n="128 r" xml:id="Kasten_blau_44_6_0199.jpg"/>
    <ab facs="#facs_1_r1">
      <lb facs="#facs_1_r1l1" n="N001"/>
      <lb facs="#facs_1_r1l2" n="N002"/>vn gerligste dae die
      <lb facs="#facs_1_r1l3" n="N003"/>aser
      <lb facs="#facs_1_r1l4" n="N004"/>ewer Durchlauct 
      <lb facs="#facs_1_r1l5" n="N005"/>e</ab>
    <ab facs="#facs_1_r2">
      <lb facs="#facs_1_r2l1" n="N001"/>vlerdurchleichtighter 
      <lb facs="#facs_1_r2l2" n="N002"/>überialen ih der vndts gehe iche
      <lb facs="#facs_1_r2l3" n="N003"/>gedet meine so instanzrige gesorgnsiste
      <lb facs="#facs_1_r2l4" n="N004"/>bitte das dise vrtwps 
      <lb facs="#facs_1_r2l5" n="N005"/>anzeaß cnpe mit dema hh
      <lb facs="#facs_1_r2l6" n="N006"/> ter ersicher 
      <lb facs="#facs_1_r2l7" n="N007"/>me erechst venden möchte
      <lb facs="#facs_1_r2l8" n="N008"/>zuede vmb vom sher la
      <lb facs="#facs_1_r2l9" n="N009"/>vgez gem galleigsten liebsten
      <lb facs="#facs_1_r2l10" n="N010"/>bbis vnd hn samb so mehr
      <lb facs="#facs_1_r2l11" n="N011"/>aet venden als
      <lb facs="#facs_1_r2l12" n="N012"/>sem peltres fa
      <lb facs="#facs_1_r2l13" n="N013"/>zegenstetn son aperion
      <lb facs="#facs_1_r2l14" n="N014"/>man sagputeit nit zu zensdlen
      <lb facs="#facs_1_r2l15" n="N015"/>vnd der mie vnd meine ministio
      <lb facs="#facs_1_r2l16" n="N016"/>vernemblich aber her ülere
      <lb facs="#facs_1_r2l17" n="N017"/>durcht die alleit berhöchte rusan
      <lb facs="#facs_1_r2l18" n="N018"/>hezlunburg ueregeer fost
      <lb facs="#facs_1_r2l19" n="N019"/>sendnsfuegsig toit in andero
      <lb facs="#facs_1_r2l20" n="N020"/>ihrg allerdigst nit zu copenuirson
      <lb facs="#facs_1_r2l21" n="N021"/>so müß ihre allerlohstgendige
      <lb facs="#facs_1_r2l22" n="N022"/>hig barl m rgogs
      <lb facs="#facs_1_r2l23" n="N023"/>ndese alrigst ospotition
      <lb facs="#facs_1_r2l24" n="N024"/>nach indoch mit gerdihnlishe sudigt
      <lb facs="#facs_1_r2l25" n="N025"/>gehorsambsten asignation vnbeg
      <lb facs="#facs_1_r2l26" n="N026"/>gebem vennach aber allegste
      <lb facs="#facs_1_r2l27" n="N027"/>eur</ab>
    <pb facs="#facs_2" n="128 v" xml:id="Kasten_blau_44_6_0200.jpg"/>
    <ab facs="#facs_2_r1">
      <lb facs="#facs_2_r1l1" n="N001"/>kansein dustraw mmeso noch
      <lb facs="#facs_2_r1l2" n="N002"/>deme der pra vom ielin zue
      <lb facs="#facs_2_r1l3" n="N003"/>oberntefatem miniteris aa
      <lb facs="#facs_2_r1l4" n="N004"/>vedessesschen doshibe ds
      <lb facs="#facs_2_r1l5" n="N005"/>kon ihme bs dasin eber verweß
      <lb facs="#facs_2_r1l6" n="N006"/> die caneslanal aaendt
      <lb facs="#facs_2_r1l7" n="N007"/>vorde erkee ich mi e
      <lb facs="#facs_2_r1l8" n="N008"/>lt may emit geerhebs
      <lb facs="#facs_2_r1l9" n="N009"/>ch bekeen hie graisen hhere
      <lb facs="#facs_2_r1l10" n="N010"/>rgse we
      <lb facs="#facs_2_r1l11" n="N011"/>verdse fatffaction velewit s
      <lb facs="#facs_2_r1l12" n="N012"/>o velaugsten a hor min
      <lb facs="#facs_2_r1l13" n="N013"/>zu vohzigen das sie dero albe
