  <div>
    <pb facs="#facs_1" n="26 r" xml:id="Kasten_blau_44_11_0173.jpg"/>
    <ab facs="#facs_1_r1">
      <lb facs="#facs_1_r1l1" n="N001"/>MT
      <lb facs="#facs_1_r1l2" n="N002"/>te
      <lb facs="#facs_1_r1l3" n="N003"/>der verlribter kse
      <lb facs="#facs_1_r1l4" n="N004"/>on mu</ab>
    <ab facs="#facs_1_r2">
      <lb facs="#facs_1_r2l1" n="N001"/>Eh
      <lb facs="#facs_1_r2l2" n="N002"/>
      <lb facs="#facs_1_r2l3" n="N003"/>mich ller durchlechtigste 
      <lb facs="#facs_1_r2l4" n="N004"/>ohenerhalte in gleichmsigem ver
      <lb facs="#facs_1_r2l5" n="N005"/>tenem hiemit vnbernh vnd
      <lb facs="#facs_1_r2l6" n="N006"/>ostalt dis heinem hin m hem ze
      <lb facs="#facs_1_r2l7" n="N007"/>dachstem  nachbem dieselbe sich in
      <lb facs="#facs_1_r2l8" n="N008"/>eir heüthlang zu diskrumb michgehal
      <lb facs="#facs_1_r2l9" n="N009"/>kein vnd dienturt vahin iahr mi
      <lb facs="#facs_1_r2l10" n="N010"/>eimn hah zi kommen beschleßem haber
      <lb facs="#facs_1_r2l11" n="N011"/>sollen vlechtem mihr anberiy ve
      <lb facs="#facs_1_r2l12" n="N012"/>e
      <lb facs="#facs_1_r2l13" n="N013"/>lankhen will ob solches auß ein
      <lb facs="#facs_1_r2l14" n="N014"/>vmb die salhtlig vmi este
      <lb facs="#facs_1_r2l15" n="N015"/>eesabet zu destrumb zi vngndthe
      <lb facs="#facs_1_r2l16" n="N016"/>derosukhes skihrendem absehen geschehe
      <lb facs="#facs_1_r2l17" n="N017"/>ao erlreben wr May m min
      <lb facs="#facs_1_r2l18" n="N018"/>gndigst daß diesalbe hierimit vnde
      <lb facs="#facs_1_r2l19" n="N019"/>dst bitte habe m dem Keiser
      <lb facs="#facs_1_r2l20" n="N020"/>heirnan nchricht zu entheilen i vnd
      <lb facs="#facs_1_r2l21" n="N021"/>ero wie auch i w. Myt vers
      <lb facs="#facs_1_r2l22" n="N022"/>sie nißt solchem fall fihrend dochster
      <lb facs="#facs_1_r2l23" n="N023"/>lenchtete gedanden ain in engeren
      <lb facs="#facs_1_r2l24" n="N024"/>canhint feindersambst gost hi rr
      <lb facs="#facs_1_r2l25" n="N025"/>om it in bitt visldig dier
      <lb facs="#facs_1_r2l26" n="N026"/>  da 
      <lb facs="#facs_1_r2l27" n="N027"/>ten
      <lb facs="#facs_1_r2l28" n="N028"/>D
      <lb facs="#facs_1_r2l29" n="N029"/>E</ab>
  </div>
