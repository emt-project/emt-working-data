   <text type="letter">
      <body>
         <pb facs="#facs_152" n="87 r" xml:id="Kasten_blau_44_8_0152.jpg" ed="89 r"/>
<ab facs="#facs_152_r1">
               <lb facs="#facs_152_r1l1" n="N001"/>EMT
               <lb facs="#facs_152_r1l2" n="N002"/>ver von ro man den englaun
               <lb facs="#facs_152_r1l3" n="N003"/>E
               <lb facs="#facs_152_r1l4" n="N004"/> das devorire decdinis in her
               <lb facs="#facs_152_r1l5" n="N005"/>curhn zu fran gelaßenen gehendigen
               <lb facs="#facs_152_r1l6" n="N006"/>schreibemb
               <lb facs="#facs_152_r1l7" n="N007"/>ih aay. mein leßer haben nie befehl
               <lb facs="#facs_152_r1l8" n="N008"/> 
               <lb facs="#facs_152_r1l9" n="N009"/>er aucht zue schreiben das die den anderen
               <lb facs="#facs_152_r1l10" n="N010"/>können bedalten das ihr an hne gnädigst
               <lb facs="#facs_152_r1l11" n="N011"/>leben daß er mit dem pegermees derend
               <lb facs="#facs_152_r1l12" n="N012"/>könne mactieren umb einen dies vndt ehr
               <lb facs="#facs_152_r1l13" n="N013"/>e
               <lb facs="#facs_152_r1l14" n="N014"/>martier gnadigst bleierren werden ein danner
               <lb facs="#facs_152_r1l15" n="N015"/>i auch deßwegen bey ihrer dan nen
               <lb facs="#facs_152_r1l16" n="N016"/>kaßer vnderschanigst belaunden mit disme
               <lb facs="#facs_152_r1l17" n="N017"/>ist im ihme gehalten zweifle nit er werde
               <lb facs="#facs_152_r1l18" n="N018"/>es dueg keine rede deß gegen menemme
               <lb facs="#facs_152_r1l19" n="N019"/>ainen abdenne
               <lb facs="#facs_152_r1l20" n="N020"/>ben geben e
               <lb facs="#facs_152_r1l21" n="N021"/>sen in vn
               <lb facs="#facs_152_r1l22" n="N022"/>e
            </ab>
<ab facs="#facs_152_r2">
               <lb facs="#facs_152_r2l1" n="N001"/>e
            </ab>

      </body>
   </text>
