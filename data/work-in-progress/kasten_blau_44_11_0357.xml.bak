   <text type="letter">
   <body>
  <div>
    <pb facs="#facs_1" n="1" xml:id="Kasten_blau_44_11_0357.jpg"/>
    <ab facs="#facs_1_r1">
      <lb facs="#facs_1_r1l1" n="N001"/>e</ab>
    <ab facs="#facs_1_r2">
      <lb facs="#facs_1_r2l1" n="N001"/>ma 1
      <lb facs="#facs_1_r2l2" n="N002"/>ech
      <lb facs="#facs_1_r2l3" n="N003"/>verwiltibtem agseri
      <lb facs="#facs_1_r2l4" n="N004"/>lono v
      <lb facs="#facs_1_r2l5" n="N005"/>bantworthing dersalben schreibens
      <lb facs="#facs_1_r2l6" n="N006"/>vnd ch vom 10 a
      <lb facs="#facs_1_r2l7" n="N007"/>sceb
      <lb facs="#facs_1_r2l8" n="N008"/> </ab>
    <ab facs="#facs_1_r3">
      <lb facs="#facs_1_r3l1" n="N001"/>aler durchleüchtigster
      <lb facs="#facs_1_r3l2" n="N002"/>ir Mt mz erstalte heimnt
      <lb facs="#facs_1_r3l3" n="N003"/>dank vndrkhstem dank das die
      <lb facs="#facs_1_r3l4" n="N004"/>selte wan hie minem auch die
      <lb facs="#facs_1_r3l5" n="N005"/>hnad det allerchöchstem glüklich hurmt 
      <lb facs="#facs_1_r3l6" n="N006"/>gelegten geburkt, wan abermahlt mit
      <lb facs="#facs_1_r3l7" n="N007"/>so eldesen empresoion endiso
      <lb facs="#facs_1_r3l8" n="N008"/>zu pratulin genuhm wollen mit
      <lb facs="#facs_1_r3l9" n="N009"/>gehorsambster bilt mich immenfankh
      <lb facs="#facs_1_r3l10" n="N010"/>in de döcht schäkbahrem gneden die
      <lb facs="#facs_1_r3l11" n="N011"/>arhaltem ölich n überigenit
      <lb facs="#facs_1_r3l12" n="N012"/>mich alle gelagenheiten aisenst er
      <lb facs="#facs_1_r3l13" n="N013"/>vermem vnd iniglich conoolirien vnbey
      <lb facs="#facs_1_r3l14" n="N014"/>demo golste befahle vnd gemüths me
      <lb facs="#facs_1_r3l15" n="N015"/>ennen meit orkhs zu befolgene
      <lb facs="#facs_1_r3l16" n="N016"/>vnman also ehnmmangele auch
      <lb facs="#facs_1_r3l17" n="N017"/>ein solchet in der in e Maysti
      <lb facs="#facs_1_r3l18" n="N018"/>Ma gndigstem vndt vermlten
      <lb facs="#facs_1_r3l19" n="N019"/>her hog vennschwgischer angelegen
      <lb facs="#facs_1_r3l20" n="N020"/>heit det ranzo halber wethetig
      <lb facs="#facs_1_r3l21" n="N021"/>di erweisem gestalt ich mennen he
      <lb facs="#facs_1_r3l22" n="N022"/>santem hi vagenfturg solchen mnt
      <lb facs="#facs_1_r3l23" n="N023"/></ab>
    <pb facs="#facs_2" n="2" xml:id="Kasten_blau_44_11_0358.jpg"/>
    <ab facs="#facs_2_r1">
      <lb facs="#facs_2_r1l1" n="N001"/></ab>
    <ab facs="#facs_2_r2">
      <lb facs="#facs_2_r2l1" n="N001"/>letin fe fet</ab>
    <ab facs="#facs_2_r3">
      <lb facs="#facs_2_r3l1" n="N001"/> 
      <lb facs="#facs_2_r3l2" n="N002"/>bemitt gedüglich inotrmint habe
      <lb facs="#facs_2_r3l3" n="N003"/>r. My m recommanclir
      <lb facs="#facs_2_r3l4" n="N004"/>ich meim kambt anligenheit vngen durt
      <lb facs="#facs_2_r3l5" n="N005"/>verhogthumbe kinburg varmit ich
      <lb facs="#facs_2_r3l6" n="N006"/>da fallt dernmahlm mut liged
      <lb facs="#facs_2_r3l7" n="N007"/>lih comsolmit werdem möge nachmah
      <lb facs="#facs_2_r3l8" n="N008"/>auss alleeinstandigste vnd venhen
      <lb facs="#facs_2_r3l9" n="N009"/>mit all schuldigstem respect
      <lb facs="#facs_2_r3l10" n="N010"/>lislidi de z ma 
      <lb facs="#facs_2_r3l11" n="N011"/>doch
      <lb facs="#facs_2_r3l12" n="N012"/>e
      <lb facs="#facs_2_r3l13" n="N013"/>d
      <lb facs="#facs_2_r3l14" n="N014"/>iien ua 
      <lb facs="#facs_2_r3l15" n="N015"/></ab>
  </div>
</body>

