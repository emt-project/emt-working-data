   <text type="letter">
   <body>
  <div>
    <pb facs="#facs_1" n="34 r" xml:id="Kasten_blau_44_11_0232.jpg"/>
    <ab facs="#facs_1_r1">
      <lb facs="#facs_1_r1l1" n="N001"/>EMT
      <lb facs="#facs_1_r1l2" n="N002"/>
      <lb facs="#facs_1_r1l3" n="N003"/>verwedber layserin
      <lb facs="#facs_1_r1l4" n="N004"/>conom </ab>
    <ab facs="#facs_1_r2">
      <lb facs="#facs_1_r2l1" n="N001"/>E
      <lb facs="#facs_1_r2l2" n="N002"/>e durchlmhtgste so
      <lb facs="#facs_1_r2l3" n="N003"/>r Msg agl instiltte ich kin
      <lb facs="#facs_1_r2l4" n="N004"/> so gndigse bebegug n
      <lb facs="#facs_1_r2l5" n="N005"/>wagem det in Vorngm menkh durub
      <lb facs="#facs_1_r2l6" n="N006"/>gelegten vohem faster neimb b nahman
      <lb facs="#facs_1_r2l7" n="N007"/>baterun himnt ganten gehorsambst da
      <lb facs="#facs_1_r2l8" n="N008"/>vnrbendenstem dauk habe auch mih
      <lb facs="#facs_1_r2l9" n="N009"/>kem by dero desfallt ahn mich
      <lb facs="#facs_1_r2l10" n="N010"/>abgelaßmem ostem be schreiben
      <lb facs="#facs_1_r2l11" n="N011"/>vomn 2  du nachstehim ahngelagtem
      <lb facs="#facs_1_r2l12" n="N012"/>to vmb dieselbe mi wegen
      <lb facs="#facs_1_r2l13" n="N013"/>den vrrgeschlagenem durnkh mit
      <lb facs="#facs_1_r2l14" n="N014"/>der se kmbestein hn carma vnd
      <lb facs="#facs_1_r2l15" n="N015"/>das kinen gebkentemn z aacleme
      <lb facs="#facs_1_r2l16" n="N016"/>  ohn hiuatenwen genuchem
      <lb facs="#facs_1_r2l17" n="N017"/>wollen i bet mihmen vnh vermehrn
      <lb facs="#facs_1_r2l18" n="N018"/>vnd kan e. kysy. may die
      <lb facs="#facs_1_r2l19" n="N019"/>vnkhste versichun entheite rlis
      <lb facs="#facs_1_r2l20" n="N020"/>dero glstem befahl ich in dieser
      <lb facs="#facs_1_r2l21" n="N021"/>chleicht allen anderem vegebenheiten
      <lb facs="#facs_1_r2l22" n="N022"/>volnbiche vnd ehe confident much
      <lb facs="#facs_1_r2l23" n="N023"/>dardurch mnme wundig nachem ir
      <lb facs="#facs_1_r2l24" n="N024"/>ege e mit der grsest
      <lb facs="#facs_1_r2l25" n="N025"/>fendt angelagen sein laßen wurnde</ab>
    <pb facs="#facs_2" n="34 v" xml:id="Kasten_blau_44_11_0233.jpg"/>
    <ab facs="#facs_2_r1">
      <lb facs="#facs_2_r1l1" n="N001"/>ilich wie ich aber dab inige so
      <lb facs="#facs_2_r1l2" n="N002"/>drwselben der amodenesisher ir
      <lb facs="#facs_2_r1l3" n="N003"/>santer diserthalb übergeben halt
      <lb facs="#facs_2_r1l4" n="N004"/>vnd wermit dieselbe sich gudt be
      <lb facs="#facs_2_r1l5" n="N005"/>beiser ersagtem b du nicht bey
      <lb facs="#facs_2_r1l6" n="N006"/>gelegt gefunden, also habe es eie
      <lb facs="#facs_2_r1l7" n="N007"/>mit gehorsambst ein vnd im
      <lb facs="#facs_2_r1l8" n="N008"/>goser befehl daruber nahn we
      <lb facs="#facs_2_r1l9" n="N009"/>warkhem solln i in die foster vnder
      <lb facs="#facs_2_r1l10" n="N010"/>kgleit verharnd vistli
      <lb facs="#facs_2_r1l11" n="N011"/> zu dul 1
      <lb facs="#facs_2_r1l12" n="N012"/>E 
      <lb facs="#facs_2_r1l13" n="N013"/>EMT</ab>
  </div>
</body>

