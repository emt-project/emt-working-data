   <text type="letter">
   <body>
  <div>
    <pb facs="#facs_1" n="31 r" xml:id="Kasten_blau_44_11_0343.jpg"/>
    <ab facs="#facs_1_r1">
      <lb facs="#facs_1_r1l1" n="N001"/>de manti rn 168
      <lb facs="#facs_1_r1l2" n="N002"/>ewr
      <lb facs="#facs_1_r1l3" n="N003"/>vermitilter serim
      <lb facs="#facs_1_r1l4" n="N004"/>leonoa
      <lb facs="#facs_1_r1l5" n="N005"/>se aenheuigen schreiben
      <lb facs="#facs_1_r1l6" n="N006"/>bantwethn eer scharbens
      <lb facs="#facs_1_r1l7" n="N007"/>von 16te f</ab>
    <ab facs="#facs_1_r2">
      <lb facs="#facs_1_r2l1" n="N001"/>
      <lb facs="#facs_1_r2l2" n="N002"/>len durchleüchtigster
      <lb facs="#facs_1_r2l3" n="N003"/> Mays vg audigste dihterm
      <lb facs="#facs_1_r2l4" n="N004"/>vom 20 nachsthingeligten metht
      <lb facs="#facs_1_r2l5" n="N005"/>Mb habe ich mit all geheinmundnr
      <lb facs="#facs_1_r2l6" n="N006"/>nenention wohlgelifeet erhalten vnd
      <lb facs="#facs_1_r2l7" n="N007"/>daraust nit nihrem ersahm vab
      <lb facs="#facs_1_r2l8" n="N008"/>dieselbe mi in dem eiischem vahl
      <lb facs="#facs_1_r2l9" n="N009"/>wesen vnsernt bundrn vnd kern
      <lb facs="#facs_1_r2l10" n="N010"/>valtnasem daalt  vemahlungt
      <lb facs="#facs_1_r2l11" n="N011"/>sachem nicht weniger vnserer frn
      <lb facs="#facs_1_r2l12" n="N012"/>vchwestern der venwillabter lanigin
      <lb facs="#facs_1_r2l13" n="N013"/>in venm mla angelagenheit
      <lb facs="#facs_1_r2l14" n="N014"/>karnkh des aunburgischem cefion
      <lb facs="#facs_1_r2l15" n="N015"/>geschifte halter di venmmle gdt
      <lb facs="#facs_1_r2l16" n="N016"/>gmihem wollen vihn erstte
      <lb facs="#facs_1_r2l17" n="N017"/>e M a ich sein alero
      <lb facs="#facs_1_r2l18" n="N018"/>gegen noch behorendet gnedigsteit
      <lb facs="#facs_1_r2l19" n="N019"/>vertanwen ihnondeist geheind
      <lb facs="#facs_1_r2l20" n="N020"/>damb vnd schleich ian endese
      <lb facs="#facs_1_r2l21" n="N021"/>vn dem zi mi ahngwesmen
      <lb facs="#facs_1_r2l22" n="N022"/>keist dncht doßlnkh schrahen vn
      <lb facs="#facs_1_r2l23" n="N023"/>lichst aber dasigem vohlge scheftet</ab>
    <pb facs="#facs_2" n="31 v" xml:id="Kasten_blau_44_11_0344.jpg"/>
    <ab facs="#facs_2_r1">
      <lb facs="#facs_2_r1l1" n="N001"/>E</ab>
    <ab facs="#facs_2_r2">
      <lb facs="#facs_2_r2l1" n="N001"/> ae
      <lb facs="#facs_2_r2l2" n="N002"/> vnd sie achir mie hntherim
      <lb facs="#facs_2_r2l3" n="N003"/>belebte gndigste pratulation
      <lb facs="#facs_2_r2l4" n="N004"/>gehorsambst hin danken</ab>
    <ab facs="#facs_2_r3">
      <lb facs="#facs_2_r3l1" n="N001"/>hin ohnfang fast einstlich ahngescher
      <lb facs="#facs_2_r3l2" n="N002"/>m volhembt aber auch edien
      <lb facs="#facs_2_r3l3" n="N003"/>lüthung der alleigender verlliher
      <lb facs="#facs_2_r3l4" n="N004"/>handt vnd fast sichtlehem alle
      <lb facs="#facs_2_r3l5" n="N005"/>albesem beystandt vnserend brndenen
      <lb facs="#facs_2_r3l6" n="N006"/>vnd dren b vnd deutschmeister
      <lb facs="#facs_2_r3l7" n="N007"/>v vlelt einer einmükhige
      <lb facs="#facs_2_r3l8" n="N008"/>postulation erfolgten e geden
      <lb facs="#facs_2_r3l9" n="N009"/>lihem aufschlag die vmbstündtliche
      <lb facs="#facs_2_r3l10" n="N010"/>nachricht egeschikt worden sein werdt
      <lb facs="#facs_2_r3l11" n="N011"/>also habe mit er Mayst ma
      <lb facs="#facs_2_r3l12" n="N012"/>mich darüber billig vom kirchen zu
      <lb facs="#facs_2_r3l13" n="N013"/>enfrewen gestalt, dam auch mich
      <lb facs="#facs_2_r3l14" n="N014"/>liebeig nicht wenig consolweirt vas
      <lb facs="#facs_2_r3l15" n="N015"/>ved kgsens mu sich der sachern
      <lb facs="#facs_2_r3l16" n="N016"/>bis dahin so gnndigst vnd gerosmit
      <lb facs="#facs_2_r3l17" n="N017"/>kiglich angenohnen haben vnd noch
      <lb facs="#facs_2_r3l18" n="N018"/>dem eie vom sehle vom Dükigem
      <lb facs="#facs_2_r3l19" n="N019"/>enstaildetem vndrechstem bericht in
      <lb facs="#facs_2_r3l20" n="N020"/>solchem alleruldeste lispopition vnd
      <lb facs="#facs_2_r3l21" n="N021"/>engungin lebung aller beschwehr
      <lb facs="#facs_2_r3l22" n="N022"/>mißem beim löbste vuh vamit</ab>
    <pb facs="#facs_3" n="32 r" xml:id="Kasten_blau_44_11_0345.jpg"/>
    <ab facs="#facs_3_r1">
      <lb facs="#facs_3_r1l1" n="N001"/> wab zeilastem verhabendet</ab>
