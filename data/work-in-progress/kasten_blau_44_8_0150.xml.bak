   <text type="letter">
      <body>
         <pb facs="#facs_150" n="86 r" xml:id="Kasten_blau_44_8_0150.jpg" ed="88 r"/>
<ab facs="#facs_150_r1">
               <lb facs="#facs_150_r1l1" n="N001"/>E
               <lb facs="#facs_150_r1l2" n="N002"/>dini
               <lb facs="#facs_150_r1l3" n="N003"/>EMT
               <lb facs="#facs_150_r1l4" n="N004"/>ereis von her man der lam her lebt
               <lb facs="#facs_150_r1l5" n="N005"/> der coade lengesen her
               <lb facs="#facs_150_r1l6" n="N006"/>ufs die das erles ein beygen
               <lb facs="#facs_150_r1l7" n="N007"/>landige schreibe
               <lb facs="#facs_150_r1l8" n="N008"/>neben überschlder er la das dener vor dem
               <lb facs="#facs_150_r1l9" n="N009"/>molar und die laxerig darbeyen das er luden
               <lb facs="#facs_150_r1l10" n="N010"/>hehen kommen der seident halt mit großen
               <lb facs="#facs_150_r1l11" n="N011"/>imenna gegen l des gericht daro er mi si
               <lb facs="#facs_150_r1l12" n="N012"/>mand darumb weiß in die capig dese
               <lb facs="#facs_150_r1l13" n="N013"/>morgen mit angener vndt abgeschreiben dero
               <lb facs="#facs_150_r1l14" n="N014"/>mit er d alle den dankh dauon haben
               <lb facs="#facs_150_r1l15" n="N015"/>halleinen auf deß haben in auch ihren d
               <lb facs="#facs_150_r1l16" n="N016"/>mein laßer befohlen er  huen heibanung
               <lb facs="#facs_150_r1l17" n="N017"/>das die dem camberg emen behauten dar
               <lb facs="#facs_150_r1l18" n="N018"/>ihre reysen ihme gnadigst erlauben daß ihr
               <lb facs="#facs_150_r1l19" n="N019"/>mit dem gemeister deme etier
               <lb facs="#facs_150_r1l20" n="N020"/>vmb einmendauß auch her ganz erb gleich
               <lb facs="#facs_150_r1l21" n="N021"/>olaidiren werden er köne ich dung deßwegen
               <lb facs="#facs_150_r1l22" n="N022"/>bey ihre dan meinen lezter vnterhänigen so
               <lb facs="#facs_150_r1l23" n="N023"/>euer Lieb
            </ab>
